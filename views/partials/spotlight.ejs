<div class="container-fluid text-center">
  <div class="container">
    <div class="row">
        <!--START-->
        <% blogs.forEach(function(Blog){ %>
          <a href="/blog/spotlight/<%= Blog._id %>" class="col-lg-4 p-4" id="blog-div-hide" style="display:none">
            <img src="<%=Blog.thumbSpot%>" alt="<%=Blog.seoSpotlight%>"onload="this.parentNode.style.display = 'inline'" class="img-fluid">
          <h2 class="pt-2"><%=Blog.titleSpot %></h2>
          <p class="date"><%=Blog.dateSpot%></p>
          <p class="date"><%=Blog.descSpot%></p>
          </a>
        <!--</a>-->
        <% }); %>
        </div>
        <!--END-->
      </div>
  </div>